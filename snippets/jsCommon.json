{
  "fengtao-router": {
    "prefix": "ftrouter",
    "body": [
      "import Layout from '@/layout'",
      "",
      "const routeName = {",
      "  path: '/',",
      "  component: Layout,",
      "  alwaysShow: true,",
      "  meta: { title: '', icon: '', affix: true, breadcrumb: false, accessPermission: true },",
      "  redirect: '',",
      "  children: [",
      "    {",
      "      path: '/',",
      "      component: () => import(''),",
      "      name: '',",
      "      alwaysShow: true,",
      "      meta: { title: '', icon: '', affix: true, accessPermission: true },",
      "      children: [",
      "        {",
      "          path: '/',",
      "          component: () => import(''),",
      "          name: '',",
      "          meta: { title: '', icon: '', affix: true, accessPermission: true, refreshBtn: true }",
      "        }",
      "      ]",
      "    }",
      "  ]",
      "}",
      "",
      "export default routeName",
      ""
    ],
    "description": "base-route"
  },
  "apiget": {
    "prefix": "apiget",
    "body": [
      "/**",
      " * @param params[]",
      " * @returns ",
      " */",
      "export function apiName(params) {",
      "  return request({",
      "    url: '',",
      "    method: 'get',",
      "    params",
      "  })",
      "}"
    ],
    "description": "apiget"
  },
  "apipost": {
    "prefix": "apipost",
    "body": [
      "/**",
      " * @param data[]",
      " * @returns ",
      " */",
      "export function apiName(data) {",
      "  return request({",
      "    url: '',",
      "    method: 'post',",
      "    data",
      "  })",
      "}"
    ],
    "description": "apipost"
  }
}
